   "source": [
    "def format_phone(x):\n",
    "    if len(x) == 10:\n",
    "        return f\"{x[0:3]}-{x[3:6]}-{x[6:10]}\"\n",
    "    else:\n",
    "        return x  # or return None\n",
    "\n",
    "df['Phone_Number'] = df['Phone_Number'].apply(format_phone)\n",
    "df"
   ]
