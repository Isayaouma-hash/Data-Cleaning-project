   "source": [
    "# Clean the Paying Customer column\n",
    "\n",
    "df['Paying Customer'] = df['Paying Customer'].astype(str).str.strip().str.upper()\n",
    "\n",
    "df['Paying Customer'] = df['Paying Customer'].replace({\n",
    "    'YES': 'Yes',\n",
    "    'Y': 'Yes',\n",
    "    'TRUE': 'Yes',\n",
    "    'T': 'Yes',\n",
    "    \n",
    "    'NO': 'No',\n",
    "    'N': 'No',\n",
    "    'FALSE': 'No',\n",
    "    'F': 'No'\n",
    "})\n",
    "\n",
    "# Replace invalid entries or \"nan\" with No (optional)\n",
    "df['Paying Customer'] = df['Paying Customer'].apply(lambda x: 'No' if x not in ['Yes', 'No'] else x)\n",
    "\n",
    "df['Paying Customer'].unique()\n",
    "df"
   ]
  },
