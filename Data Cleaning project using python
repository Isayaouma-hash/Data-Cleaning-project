   "source": [
    "# Clean the Do_Not_Contact column\n",
    "\n",
    "df['Do_Not_Contact'] = df['Do_Not_Contact'].astype(str).str.strip().str.upper()\n",
    "\n",
    "df['Do_Not_Contact'] = df['Do_Not_Contact'].replace({\n",
    "    'YES': 'Yes',\n",
    "    'Y': 'Yes',\n",
    "    'TRUE': 'Yes',\n",
    "    'T': 'Yes',\n",
    "\n",
    "    'NO': 'No',\n",
    "    'N': 'No',\n",
    "    'FALSE': 'No',\n",
    "    'F': 'No'\n",
    "})\n",
    "\n",
    "# Replace invalid entries or \"nan\" with No\n",
    "df['Do_Not_Contact'] = df['Do_Not_Contact'].apply(lambda x: 'No' if x not in ['Yes', 'No'] else x)\n",
    "\n",
    "df['Do_Not_Contact'].unique()\n",
    "df"
   ]
