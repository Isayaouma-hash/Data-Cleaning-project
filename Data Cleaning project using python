   "source": [
    "def split_address(addr):\n",
    "    parts = [p.strip() for p in str(addr).split(',')]\n",
    "    \n",
    "    street = \"\"\n",
    "    state = \"\"\n",
    "    zip_code = \"\"\n",
    "    \n",
    "    if len(parts) == 3:\n",
    "        street, state, zip_code = parts\n",
    "    elif len(parts) == 2:\n",
    "        street, state = parts\n",
    "    elif len(parts) == 1:\n",
    "        street = parts[0]\n",
    "    \n",
    "    return pd.Series([street, state, zip_code])\n",
    "\n",
    "df[['Street', 'State', 'Zip']] = df['Address'].apply(split_address)\n",
    "df"
   ]
  },
